---
import Layout from "../../layouts/Layout.astro";
// eslint-disable-next-line @typescript-eslint/ban-ts-comment
// @ts-ignore Astro typing issue in lints
import DogsListView from "../../components/dogs/DogsListView.tsx";

const { auth } = Astro.locals;
const isAuthenticated = !!auth?.user;
---

<Layout title="Psy - Lista i wyszukiwanie">
  <div class="container mx-auto px-4 py-8">
    <DogsListView client:load isAuthenticated={isAuthenticated} />
  </div>
</Layout>
